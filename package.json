{
  "name": "tap-outline",
  "displayName": "TAP Outline",
  "publisher": "bessarabov",
  "description": "Outline view for TAP (Test Anything Protocol) result files",
  "icon": "images/icon.png",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Testing",
    "Other"
  ],
  "keywords": [
    "tap",
    "test anything protocol",
    "outline",
    "testing"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bessarabov/tap-outline.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bessarabov/tap-outline/issues"
  },
  "homepage": "https://github.com/bessarabov/tap-outline",
  "activationEvents": [
    "onLanguage:tap"
  ],
  "contributes": {
    "languages": [
      {
        "id": "tap",
        "aliases": [
          "Test Anything Protocol",
          "TAP"
        ],
        "extensions": [
          ".tap"
        ]
      }
    ],
    "grammars": [
      {
        "language": "tap",
        "scopeName": "source.tap",
        "path": "./syntaxes/tap.tmLanguage.json"
      }
    ]
  },
  "files": [
    "out",
    "syntaxes",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "images"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "watch": "tsc -watch -p .",
    "vscode:prepublish": "npm run compile",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest"
  },
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    },
    "virtualWorkspaces": {
      "supported": true
    }
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.90.0",
    "@vscode/vsce": "^3.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "main": "./out/extension.js"
}
